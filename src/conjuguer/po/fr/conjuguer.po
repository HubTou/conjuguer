#: main.py
#, python-format
msgid "usage: conjuguer [--debug] [--help|-?] [--locale LANG] [--version]"
msgstr "usage: conjuguer [--debug] [--help|-?] [--locale LANGUE] [--version]"

msgid "[-c|--columns NUMBER] [-n|--nocolor] [-A|--ABU] [-D|--DELA]"
msgstr "[-c|--columns NOMBRE] [-n|--nocolor] [-A|--ABU] [-D|--DELA]"

msgid "[-d|--dictionary PATH]"
msgstr "[-d|--dictionary CHEMIN]"

msgid "[--] verb [...]"
msgstr "[--] verbe [...]"

msgid "--------------------  -----------------------------------------------------"
msgstr "----------------------  -----------------------------------------------------"

msgid "-c|--columns NUM      Choose number of columns to display between 1, 2 or 4"
msgstr "-c|--columns NOMBRE     Choix du nombre de colonnes à afficher entre 1, 2 ou 4"

msgid "-d|--dictionary PATH  Select a specific dictionary"
msgstr "-d|--dictionary CHEMIN  Selectionne le dictionnaire spécifié"

msgid "-n|--nocolor          Disable color output"
msgstr "-n|--nocolor            Désactive la colorisation"

msgid "-D|--DELA             Enable DELA format output"
msgstr "-D|--DELA               Active l'affichage au format DELA"

msgid "-A|--ABU              Enable ABU format output"
msgstr "-A|--ABU                Active l'affichage au format ABU"

msgid "--debug               Enable debug mode"
msgstr "--debug                 Active le mode de débogage"

msgid "--help|-?             Print usage and this help message and exit"
msgstr "--help|-?               Affiche l'usage de la commande, cette aide et quitte"

msgid "--locale LANG         Override environment to select another language"
msgstr "--locale LANGUE         Force la langue utilisée"

msgid "--version             Print version and exit"
msgstr "--version               Affiche la version de la commande et quitte"

msgid "--                    Options processing terminator"
msgstr "--                      Marqueur de fin d'options"

msgid "DICTPATH directory"
msgstr "Répertoire du DICTPATH"

msgid "not found"
msgstr "non trouvé"

msgid "None of the directories specified in DICTPATH found"
msgstr "Aucun des répertoires spécifiés dans le DICTPATH n'a été trouvé"

msgid "Dictionary pathname doesn't exist"
msgstr "Le chemin d'accès au dictionnaire n'existe pas"

msgid "The selected dictionary doesn't seem to be of ABU or DELA type"
msgstr "Le dictionnaire sélectionné ne semble pas être du type ABU ou DELA"

msgid "Syntax error"
msgstr "Erreur de syntaxe"

msgid "Option -c/--columns is expecting an integer argument"
msgstr "L'option -c/--columns nécessite un argument de type entier"

msgid "Option -c/--columns is expecting 1, 2 or 4 columns"
msgstr "L'option -c/--columns nécessite un nombre de colonnes entre 1, 2 ou 4"

msgid "Option -d/--dictionary is expecting a valid pathname"
msgstr "L'option -d/--dictionary nécessite un chemin d'accès valide"

msgid "time"
msgstr "temps"

msgid "lines"
msgstr "lignes"

msgid "conjugations"
msgstr "conjugaisons"

msgid "Key"
msgstr "Clé"

msgid "Line"
msgstr "Ligne"

msgid "More than one key found for"
msgstr "Plus d'une clé trouvée pour"

msgid "Only considering the first one"
msgstr "Seule la première est prise en compte"

msgid "Infinitif passé not found for"
msgstr "Passé de l'infinitif non trouvé pour"

msgid "auxiliary"
msgstr "auxiliaire"

msgid "1st group"
msgstr "1er groupe"

msgid "2nd group"
msgstr "2ème groupe"

msgid "3rd group"
msgstr "3ème groupe"

msgid "unknown group"
msgstr "groupe inconnu"

msgid "model for verbs like"
msgstr "modèle des verbes en"

msgid "conjugated like"
msgstr "conjugué comme"

msgid "Conjugation tables for"
msgstr "Tableau de conjugaisons du verbe"

msgid "Unknown inflected dictionary format"
msgstr "Format de dictionnaire fléchi inconnu"

msgid "conjuguer expects at least one argument"
msgstr "conjuguer nécessite au moins un argument"

msgid "is not in the dictionary used"
msgstr "n'est pas dans le dictionnaire utilisé"

msgid "If it really exists, it would be"
msgstr "S'il existe réellement, il serait du"

